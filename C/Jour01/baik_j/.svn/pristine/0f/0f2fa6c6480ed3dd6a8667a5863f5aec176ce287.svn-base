char	my_next_char(char c)
{
  if((c > 64 && c < 91) || (c > 96 && c < 123))
    {
      if(c == 65)
	{
	  c = 90;
	  return c;
	}
      else if(c == 97)
	{
	  c = 122;
	  return c;
	}
      else
	{
	  c = c - 1;
	  return c;
	}
    }
  else
    {
      return '\n';
    }
}
