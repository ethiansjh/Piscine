void	my_putchar(char c);

int	*my_fill_tab()
{
  static int	tab[100];
  int	i;

  for (i = 0; i != 100; i = i + 1)
    {
      tab[i] = i + 1;
    }
  return (tab);
}

int main()
{
  int	tab[100] = {0};
  my_putchar(my_fill_tab() + '0');
  my_putchar('\n');
}
